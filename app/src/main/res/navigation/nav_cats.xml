<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_cats"
    app:startDestination="@id/catsFragment">

    <fragment
        android:id="@+id/catsFragment"
        android:name="com.example.appwithcats.ui.cats.CatsFragment"
        android:label="CatsFragment"
        tools:layout="@layout/fragment_cats">
        <action
            android:id="@+id/action_catsFragment_to_catFragment"
            app:destination="@id/catFragment" />


    </fragment>
    <fragment
        android:id="@+id/catFragment"
        android:name="com.example.appwithcats.ui.cats.CatFragment"
        android:label="fragment_cat"
        tools:layout="@layout/fragment_cat" >
        <argument
            android:name="urlCat"
            app:argType="string"
            app:nullable="false"/>
        <action
            android:id="@+id/action_catFragment_to_catsFragment"
            app:destination="@id/catsFragment" />
    </fragment>

</navigation>